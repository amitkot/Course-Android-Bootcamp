<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.amitkot.android.processes"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.amitkot.android.processes.MainProcessesActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.amitkot.android.processes.AsyncTaskDemo"
            android:label="@string/title_activity_async_task_demo" >
        </activity>
        <activity
            android:name="com.amitkot.android.processes.SimpleAsyncTask"
            android:label="@string/title_activity_simple_async_task" >
        </activity>
        <activity
            android:name="com.amitkot.android.processes.WorkerThreadActivity"
            android:label="@string/title_activity_worker_thread" >
        </activity>
    </application>

</manifest>